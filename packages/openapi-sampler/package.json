{
  "name": "@har-sdk/openapi-sampler",
  "version": "0.0.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeuraLegion/har-sdk.git"
  },
  "author": {
    "name": "Artem Derevnjuk",
