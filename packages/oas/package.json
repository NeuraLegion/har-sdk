    "openapi-types": "^8.0.0",
    "qs": "^6.10.1",
    "semver": "^7.3.5",
    "url-template": "^2.0.8"
  },
  "devDependencies": {
    "@types/har-format": "^1.2.5",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^15.0.2",
