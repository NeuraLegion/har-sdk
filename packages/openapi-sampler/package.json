    "dist/**"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://github.com/NeuraLegion/har-sdk#readme",
  "scripts": {
    "build": "npm run clean && npm run compile && npm run copy",
    "compile": "tsc -p tsconfig.json",
    "lint": "eslint -c .eslintrc.js src",
    "clean": "rimraf ../../dist/openapi-sampler",
    "copy": "copyfiles -f ../../LICENSE package.json README.md ../../dist/openapi-sampler",
    "test": "cross-env TS_NODE_PROJECT=./tsconfig.spec.json mocha -r ts-node/register -r tsconfig-paths/register src/**/*.spec.ts"
  },
  "dependencies": {
    "faker": "^5.5.3",
    "json-pointer": "^0.6.1",
    "openapi-types": "^8.0.0",
    "randexp": "^0.5.3"
  },
  "devDependencies": {
    "@types/faker": "^5.5.3",
    "@types/json-pointer": "^1.0.30"
  }
}
