    "format": "prettier --ignore-path ../../.prettierignore --check **/*.ts",
    "test": "cross-env TS_NODE_PROJECT=tests/tsconfig.json mocha -r ts-node/register -r tsconfig-paths/register tests/**/*.spec.ts"
  },
  "keywords": [
    "oas",
    "swagger",
    "instantiator",
    "sampler",
    "faker"
  ],
  "dependencies": {
    "@har-sdk/types": "*",
    "faker": "^5.5.3",
    "json-pointer": "^0.6.1",
    "randexp": "^0.5.3",
    "tslib": "~2.2.0"
  },
  "devDependencies": {
    "@types/faker": "^5.5.3",
    "@types/json-pointer": "^1.0.30",
    "@types/node": "^14.14.45"
  }
