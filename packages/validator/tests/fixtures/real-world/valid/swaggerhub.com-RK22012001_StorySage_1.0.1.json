    "/user/": {
      "get": {
        "summary": "Get All User",
        "operationId": "get_all_user_user__get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": { "application/json": { "schema": {} } }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CreateBlogRequest": {
        "properties": {
          "userId": { "type": "string", "title": "Userid" },
          "title": { "type": "string", "title": "Title" },
          "image": { "$ref": "#/components/schemas/ImageSchema" },
          "description": { "type": "string", "title": "Description" }
        },
        "type": "object",
        "required": ["userId", "title", "image", "description"],
        "title": "CreateBlogRequest"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": { "$ref": "#/components/schemas/ValidationError" },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "ImageSchema": {
        "properties": {
          "url": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "Url"
          },
          "fileId": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "Fileid"
          }
        },
        "type": "object",
        "required": ["url", "fileId"],
        "title": "ImageSchema"
      },
      "UpdateBlogRequest": {
        "properties": {
          "image": { "$ref": "#/components/schemas/ImageSchema" },
          "title": { "type": "string", "title": "Title" },
          "description": { "type": "string", "title": "Description" }
        },
        "type": "object",
        "required": ["image", "title", "description"],
        "title": "UpdateBlogRequest"
      },
      "UpdateCommentRequest": {
        "properties": {
          "name": { "type": "string", "title": "Name" },
          "comment": { "type": "string", "title": "Comment" }
        },
        "type": "object",
        "required": ["name", "comment"],
        "title": "UpdateCommentRequest"
      },
      "UserLoginSchema": {
        "properties": {
          "email": { "type": "string", "format": "email", "title": "Email" },
          "password": { "type": "string", "title": "Password" }
        },
        "type": "object",
        "title": "UserLoginSchema"
      },
      "UserSchema": {
        "properties": {
          "name": { "type": "string", "title": "Name" },
          "email": { "type": "string", "title": "Email" },
          "password": { "type": "string", "title": "Password" },
          "image": {
            "anyOf": [
              { "$ref": "#/components/schemas/ImageSchema" },
              { "type": "null" }
            ]
          },
          "about": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "About"
          }
        },
        "type": "object",
        "required": ["name", "email", "password", "image", "about"],
        "title": "UserSchema"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": { "anyOf": [{ "type": "string" }, { "type": "integer" }] },
            "type": "array",
            "title": "Location"
          },
          "msg": { "type": "string", "title": "Message" },
          "type": { "type": "string", "title": "Error Type" }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      }
    }
  }
}
